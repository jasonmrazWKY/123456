
.rui-picker{
  height: 10vw;
  font-size: 28rpx;
  color: #000;
  display: -webkit-flex;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  padding: 0 10px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border: 1px solid #aaa;
  -webkit-border-radius: 3px;
          border-radius: 3px;
}


.box{background: #FFFFFF;margin-bottom: 24rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;position: fixed;width: 100%;height: 72rpx;z-index: 99;}
.section {position: relative;background: #FFFFFF; width: 50%;margin-left: 30rpx;}
.box input {padding: 0 0 0 10rpx;margin-top: 11rpx; border: 1px solid #CCCCCC;-webkit-border-radius: 20rpx;border-radius: 20rpx;margin-left: 81rpx;}
.section text{position: absolute;top: 10rpx;}
button{position: absolute;right: 20rpx;top: 10rpx;background: rgb(255, 172, 96);color: #FFFFFF;z-index: 99;}
.warp{background: #FFFFFF;}
.uni-list-cell{position: relative;}
.dw{position: fixed;bottom: 0;background: #FFFFFF;width: 100%;z-index: 10;}
.sl{color: #F6871E;font-weight: 600;font-size: 28rpx;}
.sl view{color: #F6871E;font-weight: 600;font-size:28rpx;}
.kg{margin-left: 30rpx;}
.text {background-color: #FFFFFF;}
.uni-triplex-row{padding: 0;}
.uni-common-mt{margin-top: 0;}
.uni-padding-wrap {width: 100%;padding:0;}
.uni-list-cell::after {
    position: absolute;
    z-index: 3;
    right: 0;
    bottom: 0;
    left: 0;
    height: 4rpx;
    content: '';
    -webkit-transform: scaleY(.5);
    -ms-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: #c8c7cc;
}
.uni-list-cell{
	padding-left: 30rpx;
	/* padding: 10upx 0 10upx 30upx; */
}

