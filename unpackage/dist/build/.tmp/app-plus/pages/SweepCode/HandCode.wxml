list-cell-db"><input class="uni-input" type="text" disabled="true" data-event-opts="{{[['input',[['__set_model',['','color','$event',[]]]]]]}}" value="{{color}}" bindinput="__e"/></view><view class="uni-list-cell-left"><view class="uni-label">尺码:</view></view><view class="uni-list-cell-db"><input class="uni-input" type="text" disabled="true" data-event-opts="{{[['input',[['__set_model',['','chima','$event',[]]]]]]}}" value="{{chima}}" bindinput="__e"/></view></view></view><view class="warp" style="margin-bottom:70px;"><t-table vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><t-tr vue-id="{{('2')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-th vue-id="{{('3')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">工序</t-th><t-th vue-id="{{('4')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">员工</t-th><t-th vue-id="{{('5')+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}">时间</t-th></t-tr><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><t-tr vue-id="{{('6-'+index)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><t-td vue-id="{{('7-'+index)+','+('6-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.worktype}}</t-td><t-td vue-id="{{('8-'+index)+','+('6-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.uname}}</t-td><block wx:if="{{item.inputtime!=null}}"><t-td vue-id="{{('9-'+index)+','+('6-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{''+item.inputtime+''}}</t-td></block><block wx:else><t-td vue-id="{{('10-'+index)+','+('6-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}">{{''+''+''}}</t-td></block></t-tr></block></t-table></view></view>